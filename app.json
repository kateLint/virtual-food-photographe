{
  "expo": {
    "name": "Virtual Food Photographer",
    "slug": "virtual-food-photographer",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "virtualfoodphotographer",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#111827"
    },
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.virtualfoodphotographer.app",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#111827"
      },
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "This app needs access to your photo library to save your favorite food photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save your favorite food photos.",
        "NSCameraUsageDescription": "This app needs access to your camera to scan menu photos and extract text."
      },
      "privacyUrl": "https://katelint.github.io/virtual-food-photographe/privacy-policy.html"
    },
    "android": {
      "package": "com.virtualfoodphotographer.app",
      "versionCode": 2,
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#111827"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#111827"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.CAMERA"
      ],
      "privacyUrl": "https://katelint.github.io/virtual-food-photographe/privacy-policy.html"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#111827"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "913fa71b-3e50-4fbb-aebc-a438535e8d8e"
      }
    },
    "supportsRTL": false
  }
}
